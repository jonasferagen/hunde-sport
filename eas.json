{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    },
    "internal-logs": {
      "distribution": "internal",
      "channel": "internal-logs",
      "env": {
        "KEEP_LOGS": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
