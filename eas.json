{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "internal-apk-dev": {
      "distribution": "internal",
      "channel": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_VARIANT": "dev"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    },
    "internal-apk-prod": {
      "distribution": "internal",
      "channel": "production",
      "android": { "buildType": "apk" }
    }
  },
  "cli": {
    "appVersionSource": "remote"
  }
}
