{
  "name": "hunde-sport",
  "main": "index.ts",
  "version": "1.0.10",
  "config": {
    "app_pkg_prod": "com.anonymous.hundesport",
    "app_pkg_dev": "com.anonymous.hundesport.dev"
  },
  "scripts": {
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "android:prebuild": "expo prebuild --platform android --clean",
    "android:logs:dev": "adb logcat -v color -v time --pid $(adb shell pidof -s $npm_package_config_app_pkg_dev) ReactNativeJS:V ReactNative:V *:S",
    "android:logs:prod": "adb logcat -v color -v time --pid $(adb shell pidof -s $npm_package_config_app_pkg_prod) ReactNativeJS:V ReactNative:V *:S",
    "android:install": "adb install -r $(ls -t dist/*.apk | head -n1)",
    "android:build": "eas build --local -p android --profile production --output ./dist/app-${npm_package_version}-prod.aab",
    "android:build-apk": "eas build --local -p android --profile internal-apk-dev --output ./dist/app-${npm_package_version}-dev.apk",
    "android:package": "adb shell pm list packages | grep hundesport && adb shell dumpsys package ${npm_package_config_app_pkg_dev} | grep version || true && adb shell dumpsys package ${npm_package_config_app_pkg_prod} | grep version || true",
    "android:open": "PKG=$npm_package_config_app_pkg_dev; adb shell pm path $PKG >/dev/null 2>&1 || PKG=$npm_package_config_app_pkg_prod; echo Launching $PKG && adb shell monkey -p $PKG -c android.intent.category.LAUNCHER 1",
    "android:stop": "PKG=$npm_package_config_app_pkg_dev; adb shell pm path $PKG >/dev/null 2>&1 || PKG=$npm_package_config_app_pkg_prod; echo Stopping $PKG && adb shell am force-stop $PKG",
    "android:clear": "PKG=$npm_package_config_app_pkg_dev; adb shell pm path $PKG >/dev/null 2>&1 || PKG=$npm_package_config_app_pkg_prod; echo Clearing $PKG && adb shell pm clear $PKG",
    "android:which": "for P in $npm_package_config_app_pkg_dev $npm_package_config_app_pkg_prod; do adb shell pm path $P >/dev/null 2>&1 && echo Found $P && adb shell dumpsys package $P | grep -iE 'version(Name|Code)'; done",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": "22.18.0",
    "npm": "11.5.2"
  },
  "dependencies": {
    "@nandorojo/galeria": "^1.2.0",
    "@react-native-community/hooks": "^100.1.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/drawer": "^7.3.9",
    "@react-navigation/native": "^7.1.6",
    "@sentry/react-native": "~6.14.0",
    "@shopify/flash-list": "^2.0.3",
    "@tamagui/animations-css": "^1.132.15",
    "@tamagui/animations-react-native": "^1.132.15",
    "@tamagui/babel-plugin": "^1.132.15",
    "@tamagui/config": "^1.132.15",
    "@tamagui/lucide-icons": "^1.132.20",
    "@tamagui/portal": "^1.132.15",
    "@tamagui/react-native-web-lite": "^1.132.15",
    "@tamagui/toast": "^1.132.20",
    "@tanstack/react-query": "^5.82.0",
    "apisauce": "^3.2.0",
    "burnt": "^0.13.0",
    "expo": "53.0.22",
    "expo-application": "~6.1.5",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-storage": "~53.0.11",
    "expo-system-ui": "~5.0.11",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~14.2.0",
    "he": "^1.2.0",
    "htmlparser2": "^10.0.0",
    "polished": "^4.3.1",
    "react": "19.1",
    "react-dom": "19.1",
    "react-freeze": "^1.0.4",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-in-app-updates": "^0.2.2",
    "react-native-logs": "^5.3.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-webview": "13.13.5",
    "tamagui": "^1.132.20",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/he": "^1.2.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "overrides": {
    "framer-motion": "^11.0.0"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
