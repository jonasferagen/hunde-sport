{
  "name": "hunde-sport",
  "main": "index.ts",
  "version": "1.0.1",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "prebuild:android": "expo prebuild --platform android --clean",
    "build:aab": "eas build --local -p android --profile production --output ./dist/app-${npm_package_version}-prod.aab",
    "build:apk": "eas build --local -p android --profile internal-apk-dev --output ./dist/app-${npm_package_version}-dev.apk",
    "install:apk": "adb install -r $(ls -t dist/*.apk | head -n1)"
  },
  "engines": {
    "node": "22.18.0",
    "npm": "11.5.2"
  },
