{
    "$schema": "https://unpkg.com/knip@latest/schema.json",
    "project": [
        "**/*.{ts,tsx}"
    ],
    "ignoreDependencies": [
        // Expo & RN modules are often loaded natively/babel
        "expo*",
        "react-native*",
        "@tamagui/*",
        "metro-minify-terser",
        "burnt"
    ],
    "ignore": [
        "android/**",
        "ios/**",
        "node_modules/**",
        "scripts/**"
    ]
}