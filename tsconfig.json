{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "config/tamagui.config.js"
  ],
  "exclude": [
    "node_modules",
    "android",
    "ios",
    ".expo",
    "build",
    "dist",
    "coverage"
  ]
}